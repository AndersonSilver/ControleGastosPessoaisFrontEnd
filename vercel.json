{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/server.js"
        },
        {
            "src": "/(.+\\.(js|css|png|jpg))",
            "dest": "/src/public/css/$1"
        }
    ]
}

